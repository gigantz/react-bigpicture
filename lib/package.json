{
	"name": "react-bigpicture",
	"version": "1.0.19",
	"description": "Parse html in your React App from any URL",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "webpack-dev-server --inline --hot --open",
		"build": "NODE_ENV=production webpack --config webpack.config.js --optimize-minimize --mode production",
		"npm:patch": "npm version patch && cp ./package.json ./lib/package.json",
		"npm:publish": "cd ./lib && npm publish"
	},
