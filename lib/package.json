		"react": "^16.6.1",
		"react-dom": "^16.6.1"
	},
	"devDependencies": {
		"babel": "^6.23.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
